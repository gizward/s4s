- log_object_view '/tools/seeds'

= "puts ' -- pages'"
%br
- Page.all.each do |o|

  = "abstract = <<EOS"
  %pre= o.abstract
  = "EOS<br/>".html_safe

  = "content = <<EOS"
  %pre= o.content
  = "EOS<br/>".html_safe

  = "styles = <<EOS"
  %pre= o.styles
  = "EOS<br/>".html_safe

  = "scripts = <<EOS"
  %pre= o.scripts
  = "EOS<br/>".html_safe

  = "Page.create(<br/>".html_safe
