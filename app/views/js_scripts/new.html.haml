- log_object_view 'js_scripts/new'

.top_actions.buttons
  .buttons_inner
    = RDisplay.cancel_form_link("/js_scripts")
    = RDisplay.submit_form_link("new_js_script")

= view_title @object, :new

= form_for(@object) do |f|

  = render :partial=>'/shared/admin/form_errors'
  = render :partial=>'/shared/admin/form_details',    :locals=>{:f=>f}
  = render :partial=>'/shared/admin/form_js_block', :locals=>{:f=>f}
          
.form_actions.buttons
  .buttons_inner
    = RDisplay.cancel_form_link("/js_scripts")
    = RDisplay.submit_form_link("new_js_script")
    
