- log_object_view "/app_settings/edit"

.top_actions.buttons
  .buttons_inner
    = RDisplay.cancel_form_link("/app_settings/#{@object.id}")
    = RDisplay.submit_form_link("edit_app_setting_#{@object.id}")

= view_title @object, :edit

= form_for(@object) do |f|

  = render :partial=>'/shared/admin/form_errors'
  = render :partial=>'form_details', :locals=>{:f=>f}
          
.form_actions.buttons
  .buttons_inner
    = RDisplay.cancel_form_link("/app_settings/#{@object.id}")
    = RDisplay.submit_form_link("edit_app_setting_#{@object.id}")
